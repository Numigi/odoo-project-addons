<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="project_stage_form" model="ir.ui.view">
        <field name="name">Project Stage: add field closed</field>
        <field name="model">project.stage</field>
        <field name="inherit_id" ref="project_stage.project_stage_default_form_view"/>
        <field name="arch" type="xml">
            <field name="fold" position="after">
                <field name="closed" string="Project Closed"/>
            </field>
        </field>
    </record>

</odoo>
