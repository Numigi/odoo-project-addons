<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="meeting_minutes_project.action_view_meeting_minutes_project" model="ir.actions.act_window">
        <field name="domain">['|',('parent_project_id', '=', active_id),('project_id', '=', active_id)]</field>
    </record>

    <record id="view_meeting_minutes_project_search" model="ir.ui.view">
        <field name="name">meeting.minutes.project.search</field>
        <field name="model">meeting.minutes.project</field>
        <field name="arch" type="xml">
            <search string="Project">
                <field name="parent_project_id"/>
                <field name="project_id"/>
            </search>
        </field>
    </record>

</odoo>
