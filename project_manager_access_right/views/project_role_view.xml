<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="project_role_form_inherit" model="ir.ui.view">
        <field name="name">Project Role Form Inherit</field>
        <field name="model">project.role</field>
        <field name="inherit_id" ref="project_role.project_role_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="is_manager"/>
            </xpath>
        </field>
    </record>
</odoo>