<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="stock_move_line_with_project_tree" model="ir.ui.view">
        <field name="name">Stock Move Line Tree: add project</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <field name="product_id" position="before">
                <field name="project_id" string="Project"/>
            </field>
        </field>
    </record>

    <record id="stock_move_line_with_project_form" model="ir.ui.view">
        <field name="name">Stock Move Line Form: add project</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="product_id" position="before">
                <field name="project_id" string="Project"/>
            </field>
        </field>
    </record>

</odoo>
